<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/analysis_navigation"
    app:startDestination="@id/cameraXFragment">
    <fragment
        android:id="@+id/cameraXFragment"
        android:name="com.example.chickcheckapp.presentation.ui.camera.CameraXFragment"
        android:label="CameraXFragment">
        <action
            android:id="@+id/action_cameraXFragment_to_resultFragment"
            app:destination="@id/resultFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.example.chickcheckapp.presentation.ui.result.ResultFragment"
        android:label="ResultFragment">
        <argument
            android:name="uri_image"
            app:argType="string" />
        <argument
            android:name="article"
            app:argType="com.example.chickcheckapp.data.remote.response.ArticleData" />
        <action
            android:id="@+id/action_resultFragment_to_cameraXFragment"
            app:destination="@id/cameraXFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/resultFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_resultFragment_to_activity_home"
            app:destination="@id/activity_home"
            app:launchSingleTop="true"
            app:popUpTo="@id/activity_home"
            app:popUpToInclusive="true" />
    </fragment>
    <activity
        android:id="@+id/activity_home"
        android:name="com.example.chickcheckapp.presentation.MainActivity"
        android:label="MainActivity" />
</navigation>